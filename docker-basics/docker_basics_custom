git clone https://github.com/jasonumiker/docker-kube-intro-workshop.git
cd docker-kube-intro-workshop
cat Dockerfile
docker build -t nginx:1.0 .
docker history nginx:1.0
docker run -p 8080:80 --name nginx nginx:1.0
curl http://localhost:8080
docker ps -a
sudo docker inspect nginx
docker rm nginx

****File System Mount for live changes****
docker run -d -p 8080:80 -v /home/ec2-user/environment/docker-basics/docker-kube-intro-workshop/index.html:/usr/share/nginx/html/index.html:ro --name nginx nginx:latest
curl http://localhost:8080
docker stop nginx && docker start nginx
curl http://localhost:8080
docker stop nginx && docker rm nginx








