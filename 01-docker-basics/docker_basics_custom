#//git clone https://github.com/jasonumiker/docker-kube-intro-workshop.git
cd nginx-ex1
cat Dockerfile
docker build -t mynginx:1.0 .
docker history mynginx:1.0
docker run -p 8080:80 --name mynginx mynginx:1.0
curl http://localhost:8080
docker ps -a
sudo docker inspect mynginx
docker rm mynginx



****File System Mount for live changes****
docker run -d -p 8080:80 -v $PWD/index.html:/usr/share/nginx/html/index.html:ro --name nginx nginx:latest
curl http://localhost:8080
docker stop nginx && docker rm nginx && docker rmi nginx


*****Docker Push Command ******
https://hub.docker.com/
docker login
docker tag mynginx:1.0 chethancmk/mynginx:1.0
docker push chethancmk/mynginx:1.0

docker rmi chethancmk/mynginx:1.0
docker rmi mynginx:1.0

docker pull chethancmk/mynginx:1.0









